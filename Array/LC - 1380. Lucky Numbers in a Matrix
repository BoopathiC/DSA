class Solution {
public:
    vector<int> luckyNumbers(vector<vector<int>>& matrix) {
        

        vector<int>ln;

        for(int i=0;i<matrix.size();i++){
            int minval=matrix[i][0];
            int mincol=0;

            for(int j=1;j<matrix[0].size();j++){
                if(matrix[i][j]<minval){
                    minval=matrix[i][j];
                    mincol=j;
                }
            }
    
        }
    }
};
