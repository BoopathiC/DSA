// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int n;
    cin>>n;
    int temp=0;
    while(n>0){
        int unit=n%10;
        n=n/10;
        temp=temp*10;
        temp=temp+unit;
    }
    cout <<temp;

    return 0;
}
